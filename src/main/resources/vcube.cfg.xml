<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>

     	<property name="hibernate.default_schema">VCUBE</property>
 		<property name="show_sql">false</property>
 		<property name="format_sql">true</property>
	    <property name="hibernate.globally_quoted_identifiers">true</property>
 		

        <mapping class="com.vmetrix.v3.logsmanagement.model.LogModule"/>
        <mapping class="com.vmetrix.v3.logsmanagement.model.LogRecord"/>
        <mapping class="com.vmetrix.v3.logsmanagement.model.LogLevelConf"/>
        <mapping class="com.vmetrix.v3.logsmanagement.model.LogSetting"/>
        
    </session-factory>
</hibernate-configuration>
